   "source": [
    "import re\n",
    "\n",
    "texts = [\n",
    "    \"2024   年 阿萨德阿萨德\",\n",
    "    \"啊速度很快2025  年 02 月 2131 阿萨德  \"\n",
    "    \"2026 年 03 月5 日 \"\n",
    "    \"2027年03月28日\"\n",
    "]\n",
    "\n",
    "for text in texts:\n",
    "    for match_p in re.finditer(\"(\\d{4})\\s*年\\s*(\\d{1,2})?\\s*月\\s*(\\d{1,2})?\\s*日?\", text):\n",
    "        print(match_p.group())"
   ]
  }
